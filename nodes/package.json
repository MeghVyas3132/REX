{
  "name": "@rex/nodes",
  "version": "1.0.0",
  "description": "Workflow nodes for REX automation platform",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    ".": {
      "types": "./index.ts",
      "require": "./index.ts",
      "import": "./index.ts",
      "default": "./index.ts"
    },
    "./registry": {
      "types": "./registry/node-registry.ts",
      "require": "./registry/node-registry.ts",
      "import": "./registry/node-registry.ts"
    },
    "./*": {
      "types": "./*/index.ts",
      "require": "./*/index.ts",
      "import": "./*/index.ts"
    }
  },
  "files": [
    "**/*.ts",
    "!**/*.test.ts"
  ],
  "dependencies": {
    "axios": "^1.7.0",
    "form-data": "^4.0.0",
    "luxon": "^3.0.0",
    "mailparser": "^3.0.0",
    "node-cron": "^3.0.0",
    "pg": "^8.0.0",
    "xlsx": "^0.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.0",
    "@types/pg": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/rex.git",
    "directory": "nodes"
  },
  "keywords": [
    "rex",
    "workflow",
    "automation",
    "nodes"
  ],
  "license": "MIT"
}
